# Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_URL_PRODUCTION=https://your-backend-domain.vercel.app/api

# Supabase Configuration (if using Supabase client-side)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
VITE_APP_NAME=Quản Lý Tiệm Bánh
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# Pagination
VITE_DEFAULT_PAGE_SIZE=10
VITE_MAX_PAGE_SIZE=100

# Cache Configuration
VITE_CACHE_DURATION=300000

# Google Services (if needed)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
